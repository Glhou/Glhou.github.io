  "description": "## How to run",
  "main": "index.js",
  "scripts": {
    "build:css": "npx postcss assets/css/tailwind.css -o assets/css/main.css",
    "watch:css": "npx tailwindcss -i assets/css/tailwind.css -o assets/css/main.css --watch",
    "dev": "concurrently \"bundle exec jekyll serve\" \"npm run watch:css\""
